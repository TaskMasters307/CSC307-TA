{
    "routes": [
      {
        "route": "/static/*",
        "headers": {
          "cache-control": "public, max-age=31536000"
        }
      },
      {
        "route": "/assets/*",
        "headers": {
          "cache-control": "public, max-age=31536000"
        }
      },
      {
        "route": "/*",
        "rewrite": "/index.html"
      }
    ],
    "mimeTypes": {
      ".js": "application/javascript",
      ".css": "text/css",
      ".json": "application/json",
      ".ico": "image/x-icon",
      ".png": "image/png"
    }
  }